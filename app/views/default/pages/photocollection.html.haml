.booyah-box
	.booyah-box
		.page-title
			%h1= @page.title
		%br/
		.booyah-box
		%br
			- if @page.content(:photo_collection).try(:photos)
				%ul
					.row
						- @page.content(:photo_collection).photos.each do |photo|
							.col-md-6
								= link_to image_tag(photo.file_url, :class => "uploaded-pictures"), photo.file_url, :target => "_blank"